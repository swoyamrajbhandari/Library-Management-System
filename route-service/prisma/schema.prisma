generator client {
  provider = "prisma-client-js"
  // output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL_ROUTES")
  
}

model APIRoutes {
  id               Int         @id @default(autoincrement())
  method           String
  route            String
  resource         String?
  action           String?

  @@unique([method, route], name: "method_route")
  @@map("api_routes")
  
}