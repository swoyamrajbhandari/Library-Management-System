generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL_CASBIN")
}

model CasbinRule {
  id    Int      @id @default(autoincrement())
  ptype String?
  v0    String?
  v1    String?
  v2    String?
  v3    String?
  v4    String?

  @@map("casbin_rule")
}

model APIRoutes {
  id               Int         @id @default(autoincrement())
  method           String
  route            String
  resource         String
  action           String

  @@map("api_routes")
  
}